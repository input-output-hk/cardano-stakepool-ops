{{ range service "internal" }}
  {{ if (not (.ID | regexMatch (env "NOMAD_ALLOC_ID"))) }}
    {{ scratch.MapSet "vars" .ID . }}
  {{ end }}
{{ end }}
{{ range $index, $service := (scratch.MapValues "vars" ) }}
  {{- if ne $index 0}},{{else}} {{end -}}
  { "address": "/ip4/{{ .NodeAddress }}/tcp/{{ .Port }}" }
{{ end -}}
